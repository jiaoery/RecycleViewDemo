<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.jixiang52002.recycleviewdemo">


    <uses-permission android:name="android.permission.INTERNET"/>
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE"/>
    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE"/>

    <application
        android:allowBackup="true"
        android:icon="@mipmap/fengche"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity" />
        <activity android:name=".LaucherActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".activity.ToolBarActivity" />
        <activity android:name=".activity.EasyRecyclerViewActivity" />
        <activity android:name=".activity.RefreshAndMoreActivity" />
        <activity android:name=".activity.MultiStyleActivity" />
        <activity android:name=".activity.HeaderFooterActivity" />
        <activity android:name=".activity.CollapsingActivity" />
        <activity android:name=".activity.StaggeredGridActivity" />
        <activity android:name=".activity.HorizontalActivity" />
        <activity android:name=".activity.StickyHeaderActivity" />
        <activity android:name=".activity.InsertActivity"></activity>
    </application>

</manifest>